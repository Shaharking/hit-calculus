מצאו את האיבר הכללי של הסדרות הבאות:
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
\left\{ 2,4,6,8,16\right\} =\begin{cases}
a_{n}=2^{n} & n\in N\\
a_{m}=2^{(m+1)} & m\in N_{0}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\left\{ -\frac{4}{3},\frac{5}{5},-\frac{6}{7},\frac{7}{9}\right\} =\begin{cases}
a_{n}=\frac{\left(-1\right)^{n}\cdot(3+n)}{1+2n} & n\in N\end{cases}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\left\{ \frac{1}{2},\frac{1}{2}+\frac{3}{4},\frac{1}{2}+\frac{3}{4}+\frac{5}{8},\frac{1}{2}+\frac{3}{4}+\frac{5}{8}+\frac{7}{16}\right\} =\begin{cases}
a_{n}=\stackrel[i=1]{n}{\sum}\frac{1+2(i-1)}{2^{i}}=\stackrel[i=1]{n}{\sum}\frac{2i-1}{2^{i}} & n\in N\end{cases}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
להוכיח את הגבול לפי ההגדרה:
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(2+\frac{1}{n}\right)=2
\]

\end_inset


\begin_inset Newline newline
\end_inset

יהי 
\begin_inset Formula $\epsilon>0$
\end_inset

 כלשהו , נוכיח כי קיים 
\begin_inset Formula $N$
\end_inset

 כך שלכל 
\begin_inset Formula $n>N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\left(2+\frac{1}{n}\right)-2\right|<\epsilon$
\end_inset


\begin_inset Newline newline
\end_inset

נבצע חישוב עזר :
\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left|2+\frac{1}{n}-2\right|=\left|\frac{1}{n}\right|\underset{n>0}{\leq}\frac{1}{n}<\epsilon\leftrightarrow\frac{1}{\epsilon}<n
\]

\end_inset


\begin_inset Newline newline
\end_inset

לכן מצאנו 
\begin_inset Formula $N=\frac{1}{\epsilon}$
\end_inset

 כך ש 
\begin_inset Formula $n>N$
\end_inset

 שמקיים 
\begin_inset Formula 
\[
\left|2+\frac{1}{n}-2\right|\leq\frac{1}{n}=\frac{1}{\frac{1}{\epsilon}}=\epsilon
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{3n+4}{2n+1}\right)\neq1
\]

\end_inset


\begin_inset Newline newline
\end_inset

צריך למצוא שקיים 
\begin_inset Formula $\epsilon>0$
\end_inset

 כך שלכל 
\begin_inset Formula $N>0$
\end_inset

 קיים 
\begin_inset Formula $n>N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\frac{3n+4}{2n+1}-1\right|\geq\epsilon$
\end_inset


\begin_inset Newline newline
\end_inset

נבצע חישוב עזר:
\begin_inset Formula 
\[
\left|\frac{3n+4}{2n+1}-1\right|=\left|\frac{3n+4-2n-1}{2n+1}\right|=\left|\frac{n-3}{2n+1}\right|\geq\left|\frac{n-3}{2n}\right|\underset{n>3}{\geq}\frac{n-3}{2n}>\epsilon\leftrightarrow\frac{n-3}{2\epsilon}>n=N
\]

\end_inset


\begin_inset Newline newline
\end_inset

נקח 
\begin_inset Formula $\epsilon_{4}=\frac{n-3}{2n}=\frac{1}{8}$
\end_inset

 קיים 
\begin_inset Formula $n=\frac{1}{\frac{2}{8}}=4$
\end_inset

 כך משתקיים 
\begin_inset Formula $\left|\frac{16}{9}-\frac{9}{9}\right|=\frac{7}{9}\geq\frac{1}{8}$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{n}{n+1}=1
\]

\end_inset


\begin_inset Newline newline
\end_inset

יהי 
\begin_inset Formula $\epsilon>0$
\end_inset

כלשהו, צריך להוכיח שקיים 
\begin_inset Formula $N>0$
\end_inset

, כך שלכל 
\begin_inset Formula $n>N$
\end_inset

מתקיים 
\begin_inset Formula $\left|\frac{n}{n+1}-1\right|<\epsilon$
\end_inset


\begin_inset Newline newline
\end_inset

נבצע חישוב עזר:
\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left|\frac{n}{n+1}-1\right|=\left|\frac{n-n-1}{n+1}\right|=\left|\frac{-1}{n+1}\right|=\underset{n>0>-1}{\frac{1}{n+1}<\frac{1}{n}}<\epsilon\leftrightarrow\frac{1}{\epsilon}<n=N
\]

\end_inset


\begin_inset Newline newline
\end_inset

נחזור לתרגיל , 
\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left|\frac{n}{n+1}-1\right|<\frac{1}{n}=\frac{1}{\frac{1}{\epsilon}}=\epsilon
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{2n+1}{4n+3}=\frac{1}{2}
\]

\end_inset


\begin_inset Newline newline
\end_inset

יהי 
\begin_inset Formula $\epsilon>0$
\end_inset

 כלשהו, צריך להוכיח שקיים 
\begin_inset Formula $N>0$
\end_inset

 כך שלכל 
\begin_inset Formula $n>N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\frac{2n+1}{4n+3}-\frac{1}{2}\right|<\epsilon$
\end_inset


\begin_inset Newline newline
\end_inset

נבצע חישוב עזר:
\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left|\frac{2n+1}{4n+3}-\frac{1}{2}\right|=\left|\frac{4n+2-4n-3}{2(4n+3)}\right|=\left|\frac{-1}{8n+6}\right|\underset{n>0}{=}\frac{1}{8n+6}<\frac{1}{8n}<\frac{1}{n}<\epsilon\leftrightarrow\frac{1}{\epsilon}<n
\]

\end_inset


\begin_inset Newline newline
\end_inset

לכן מתקיים 
\begin_inset Formula 
\[
\left|\frac{2n+1}{4n+3}-\frac{1}{2}\right|<\frac{1}{n}=\frac{1}{\frac{1}{\epsilon}}=\epsilon
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{n+8}{6n-7}\neq1
\]

\end_inset


\begin_inset Newline newline
\end_inset

קיים 
\begin_inset Formula $\epsilon>0$
\end_inset

 , כך שלכל 
\begin_inset Formula $N>0$
\end_inset

 קיים 
\begin_inset Formula $n>N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\frac{n+8}{6n-7}-1\right|>\epsilon$
\end_inset


\begin_inset Newline newline
\end_inset

נבצע חישוב עזר:
\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left|\frac{n+8}{6n-7}-1\right|=\left|\frac{n+8-6n+7}{6n-7}\right|=\left|\frac{-5n+15}{6n-7}\right|\underset{n>3}{\geq}\frac{5n-15}{6n-7}>\frac{5(n-3)}{6n}>\frac{5(n-3)}{5n}=\frac{n-3}{n}
\]

\end_inset


\begin_inset Newline newline
\end_inset

נקח 
\begin_inset Formula $n>4$
\end_inset

 , 
\begin_inset Formula $\epsilon=\frac{1}{4}$
\end_inset

 
\begin_inset Formula 
\[
\left|\frac{12}{17}-1\right|=\frac{5}{17}>\frac{1}{4}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
חשב את הגבולות:
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(5+\frac{6}{n^{2}}\right)=lim_{n\rightarrow\infty}5+lim_{n\rightarrow\infty}\frac{6}{n^{2}}=5+0=5
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{n^{2}}{n+1}-\frac{\left(n+1\right)^{2}}{n}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{3}-(n+1)^{3}}{n\left(n+1\right)}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{3}-\left(n^{3}-3n^{2}-3n+1\right)}{n(n+1)}\right)=lim_{n\rightarrow\infty}\left(\frac{3n^{2}+3n+1}{n^{2}+n}\right)
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow'\infty}\left(\frac{\bcancel{n^{2}}\left(3+\frac{3}{n}+\frac{1}{n^{2}}\right)}{\bcancel{n^{2}}\left(1+\frac{1}{n}\right)}\right)=\frac{3+0+0}{1+0}=3
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{2n^{4}-n^{2}+1}{n^{2}+9n+9n^{4}}\underset{t=n^{2}}{=}\frac{2t^{2}-t+1}{9t^{2}+t+\frac{9}{t}}=\frac{t^{2}\left(2-\frac{1}{t}+\frac{1}{t^{2}}\right)}{t^{2}\left(9+\frac{1}{t}+\frac{9}{t^{3}}\right)}=\frac{2}{9}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{n^{4}-n^{3}+n}{\left(n^{2}+2n+1\right)^{2}}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{4}-n^{3}+n}{\left(n^{2}+2n+1\right)\left(n^{2}+2n+1\right)}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{4}-n^{3}+n}{n^{4}+4n^{3}+6n^{2}+4n+1}\right)
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{n^{4}\left(1-\frac{1}{n}+\frac{1}{n^{3}}\right)}{n^{4}\left(1+\frac{4}{n}+\frac{6}{n^{2}}+\frac{4}{n^{3}}+\frac{1}{n^{4}}\right)}\right)=\frac{1}{1}=1
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{n^{4}-n^{2}}{0.01n-n^{2}}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{4}\left(1-\frac{1}{n^{2}}\right)}{n^{2}\left(\frac{0.01}{n}-1\right)}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{2}\left(1-\frac{1}{n^{2}}\right)}{\frac{0.01}{n}-1}\right)=\infty
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(n-\sqrt{n+a}\sqrt{n+b}\right)=lim_{n\rightarrow\infty}\left(\frac{n-\sqrt{n+a}\sqrt{n+b}}{1}\cdot\frac{n+\sqrt{n+a}\sqrt{n+b}}{n+\sqrt{n+a}\sqrt{n+b}}\right)=lim_{n\rightarrow\infty}\left(\frac{n^{2}-(n+a)(n+b)}{n+\sqrt{n+a}\sqrt{n+b}}\right)=
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\left(\frac{n^{2}-\left(n^{2}+nb+na+ab\right)}{n+\sqrt{n\left(1+\frac{a}{n}\right)}\sqrt{n\left(1+\frac{b}{n}\right)}}\right)=lim_{n\rightarrow\infty}\frac{-n(a+b+\frac{ab}{n})}{n(1+\sqrt{1+\frac{a}{n}}\sqrt{1+\frac{b}{n}})}=-\frac{a+b}{2}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{\sqrt{n+4}-\sqrt{n+1}}{\sqrt{n}}=lim_{n\rightarrow\infty}\left(\frac{\sqrt{n+4}-\sqrt{n+1}}{\sqrt{n}}\cdot\frac{\sqrt{n+4}+\sqrt{n+1}}{\sqrt{n+4}+\sqrt{n+1}}\right)=lim_{n\rightarrow\infty}\left(\frac{n+4-\left(n+1\right)}{\sqrt{n}\left(\sqrt{n+4}+\sqrt{n+1}\right)}\right)=
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\left(\frac{3}{\sqrt{n}\left(\sqrt{n}\sqrt{1+\frac{4}{n}}+\sqrt{n}\sqrt{1+\frac{1}{n}}\right)}\right)=lim_{n\rightarrow\infty}\frac{3}{n\left(\sqrt{1+\frac{4}{n}}+\sqrt{1+\frac{1}{n}}\right)}=0
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}n\left[\left(27+\frac{1}{n}\right)^{\frac{1}{3}}-3\right]=lim_{n\rightarrow\infty}n\left[3\left(1+\frac{1}{27n}\right)^{\frac{1}{3}}-3\right]=lim_{n\rightarrow\infty}3n\left[1+\frac{1}{3}\cdot\frac{1}{27n}+O\left(\frac{1}{n^{2}}\right)-1\right]=
\]

\end_inset


\begin_inset Formula 
\[
==lim_{n\rightarrow\infty}3n\left(\frac{1}{3}\cdot\frac{1}{27n}+O\left(\frac{1}{n^{2}}\right)\right)=\frac{1}{27}
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
מציאת הגבולות הבאים:
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{1+2+2^{2}+....+2^{n}}{1+5+5^{2}+....+5^{n}}=lim_{n\rightarrow\infty}\stackrel[n=0]{\infty}{\sum}\left(\frac{2}{5}\right)^{n}=lim_{n\rightarrow\infty}\frac{\frac{2}{5}^{n}-1}{\frac{2}{5}-1}=lim_{n\rightarrow\infty}\frac{\frac{2}{5}^{n}-1}{-\frac{3}{5}}=\frac{5}{3}\left(1-\frac{2}{5}^{n}\right)=\frac{5}{3}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{1}{n^{2}}+\frac{2}{n^{2}}+....+\frac{n-1}{n^{2}}\right)=lim_{n\rightarrow\infty}\frac{1}{n^{2}}\left(\stackrel[i=1]{n-1}{\sum}i\right)=lim_{n\rightarrow\infty}\frac{1}{n^{2}}\left(\frac{n-1(n)}{2}\right)=lim_{n\rightarrow\infty}\frac{n^{2}-n}{2n^{2}}=\frac{1}{2}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{1+3+5....+(2n+1)}{n+1}-\frac{2n+1}{2}\right)=lim_{n\rightarrow\infty}\left(\frac{1}{n+1}\left(\stackrel[i=1]{n}{\sum}\left(2n+1\right)\right)-\frac{2n+1}{2}\right)=
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\frac{1}{n+1}\left(\frac{n\left(2+(n-1)2\right)}{2}\right)-\frac{2n+1}{2}=lim_{n\rightarrow\infty}\frac{n^{2}}{n+1}-\frac{2n+1}{2}=
\]

\end_inset

 
\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\frac{2n^{2}-(2n+1)(n+1)}{2(n+1)}=lim_{n\rightarrow\infty}\frac{-3n-1}{2n+2}=-\frac{3}{2}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left[\frac{7}{10}+\frac{29}{100}+...+\frac{5^{n}+2^{n}}{10^{n}}\right]=lim_{n\rightarrow\infty}\left[\left(\frac{1}{2}+\frac{1}{5}\right)+\left(\frac{1}{2^{2}}+\frac{1}{5^{2}}\right)+....+\left(\frac{1}{2^{n}}+\frac{1}{5^{n}}\right)\right]=
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\left[\left(\frac{1}{2}+...+\frac{1}{2^{n}}\right)+\left(\frac{1}{5}+....+\frac{1}{5^{n}}\right)\right]=lim_{n\rightarrow\infty}\left(\frac{\frac{1}{2}\left(\frac{1}{2}^{n}-1\right)}{\frac{1}{2}-1}\right)+\left(\frac{\frac{1}{5}\left(\frac{1}{5}^{n}-1\right)}{\frac{1}{5}-1}\right)=
\]

\end_inset


\begin_inset Formula 
\[
=lim_{n\rightarrow\infty}\left(1-\frac{1}{2}^{n}\right)+\left(\frac{1}{\bcancel{5}}\cdot\frac{\bcancel{5}}{4}\left(1-\frac{1}{5}^{n}\right)\right)=\frac{5}{4}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{n^{2}+n!+\left(n+1\right)\left(n+1\right)!}{\left(n+2\right)!}=lim_{n\rightarrow\infty}\frac{n^{2}+n!+(n+1)^{2}n!}{n!(n+1)(n+2)}=lim_{n\rightarrow\infty}\frac{\cancel{n!}\left(2n^{2}+2n+1\right)}{\cancel{n!}\left(n^{2}+3n+2\right)}=2
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\frac{2^{n+1}+3^{n}}{2^{n}+3^{n+1}}=lim_{n\rightarrow\infty}\frac{\bcancel{3^{n}}\left(2\cdot\left(\frac{2}{3}\right)^{n}+1\right)}{\bcancel{3^{n}}\left(\left(\frac{2}{3}\right)^{n}+3\right)}=\frac{1}{3}
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
lim_{n\rightarrow\infty}\left(\frac{2^{n}+4^{n}}{2\cdot9^{n}-8^{n}}\right)=lim_{n\rightarrow\infty}\frac{9^{n}\left(\frac{2}{9}^{n}+\frac{4}{9}^{n}\right)}{9^{n}\left(2-\frac{8}{9}^{n}\right)}=\frac{0}{2}=0
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
חשב את הגבולות
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula 
\[
0=lim_{n\rightarrow\infty}\frac{-n}{n^{2}+1}\leq lim_{n\rightarrow\infty}\frac{nsin\left(n\right)}{n^{2}+1}\leq lim_{n\rightarrow\infty}\frac{n}{n^{2}+1}=\frac{n(1)}{n(n+\frac{1}{n})}=0
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
0=lim_{n\rightarrow\infty}\frac{-1}{2^{n}}\leq lim_{n\rightarrow\infty}\frac{cosn}{2^{n}}\leq lim_{n\rightarrow\infty}\frac{1}{2^{n}}=0
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
4=lim_{n\rightarrow\infty}\sqrt[n]{4^{n}}\leq lim_{n\rightarrow\infty}\sqrt[n]{3^{n}+4^{n}}\leq lim_{n\rightarrow\infty}\sqrt[n]{2\cdot4^{n}}=4
\]

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula 
\[
\frac{}{}\leq lim_{n\rightarrow\infty}\frac{\sqrt[3]{n^{2}}cos(n!)}{n+1}\leq lim_{n\rightarrow\infty}\frac{n^{\frac{2}{3}}}{n+1}=lim_{n\rightarrow\infty}\frac{n^{\frac{2}{3}}\cdot1}{n^{\frac{2}{3}}\left(n^{\frac{1}{3}}+\frac{1}{n^{\frac{2}{3}}}\right)}=0
\]

\end_inset


\end_layout

\end_deeper
